#include <iostream>
#include "edge.h"
using std::cout;
using std::endl;
int main() {
    Edge edge1(nullptr, nullptr, 1);  
    cout << "edge1.get_source(): " << edge1.get_source() << endl;
    cout << "edge1.get_destination(): " << edge1.get_destination() << endl;
    cout << "edge1.get_weight(): " << edge1.get_weight() << endl;
    return 0;
}
